<xs:schema xmlns:OBsharding-D="http://OBsharding-D.cloud/" attributeFormDefault="unqualified" elementFormDefault="qualified"
           targetNamespace="http://OBsharding-D.cloud/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:import schemaLocation="sharding_detail.xsd"/>
    <xs:element name="sharding">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="schema"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="shardingNode"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="function"/>
            </xs:sequence>
            <xs:attribute name="version" type="xs:string"/>
        </xs:complexType>
    </xs:element>
</xs:schema>